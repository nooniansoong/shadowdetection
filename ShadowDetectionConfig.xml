<?xml version="1.0" encoding="utf-8"?>

<QsConfig>
    <process>
        <!-- true, false -->
        <UseBatch>
            true
        </UseBatch>
        <Training>
            <!-- true, false -->
            <distribute0and1>
                true
            </distribute0and1>
        </Training>
        <Prediction>            
            <!-- true, false -->
            <usePrediction>
                true
            </usePrediction>
            <!--SVM for support vector machine, REG for regresion-->
            <predictionType>
                SVM
            </predictionType>            
        </Prediction>
        <Thresholds>
            <Values>
                <rValue>
                    200
                </rValue>
                <gValue>
                    200
                </gValue>
                <bValue>
                    180
                </bValue>
                <lValue>
                    180
                </lValue>
            </Values>
            <UseThresh>
                true;
            </UseThresh>
        </Thresholds>        
    </process>
    <settings>
        <Parameters>
            <!-- percents of image width -->
            <numSegments>
                16
            </numSegments>
        </Parameters>
        
        <openCL>            
