                0
            </platformid>
            <!-- index of device -->
            <deviceid>
                0
            </deviceid>    
        </openCL>
        
        <openMP>
            <!-- openMP thread number -->
            <threadNum>
                4
            </threadNum>
        </openMP>
        
        <svm>
            <!-- full file name -->
            <modelFile>
                bigModel_RemovedThree.model
            </modelFile>            
            <!-- 0=C-SVC, 1=nu-SVC, 2=one-class, 3=epsilon-SVR, 4=nu-SVR -->
