                -2.51965 
            </coefNo6>
            <coefNo7>
                1.49102 
            </coefNo7>
            <coefNo8>
                -2.45055 
            </coefNo8>
            <coefNo9>
                5.74217 
            </coefNo9>
            <coefNo10>
                -6.24504 
            </coefNo10>
            <coefNo11>
                1.18127
            </coefNo11>
            <coefNo12>
                -0.36501
            </coefNo12>            
            <Intercept>
                -0.52834
